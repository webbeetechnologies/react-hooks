{
  "name": "@bambooapp/react-hooks",
  "version": "1.0.7",
  "main": "./index.js",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "main": "./index.js",
    "module": "./index.js",
    "types": "./index.d.ts"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webbeetechnologies/bamboo-hooks.git"
  },
  "typings": "./index.d.ts",
  "packageManager": "yarn@4.1.0",
  "workspaces": [
    "./use*"
  ],
  "devDependencies": {
    "@types/eslint": "^8",
    "@types/lodash": "^4",
    "@types/react": "^18.2.45",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.10",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "typescript": "^4.9.5"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "prepare": "husky",
    "type-check": "tsc",
    "compile": "npx -p typescript tsc -p ./tsconfig.json",
    "build:types": "yarn workspaces foreach --all --include @bambooapp/react-hooks run compile --pretty --removeComments"
  }
}
